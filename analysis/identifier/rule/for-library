(library (scheme-langserver analysis identifier rule for-library)
  (export library-rules)
  (import 
    (chezscheme) 
    (ufo-match)
    (scheme-langserver util io))

(define (library-rules root-file-node current-file-node index-node)
  (let ([expression (annotation-stripped (index-node-datum/annotations index-node))])
    (match expression 
      [('library (item **1) rest ... ) 
        (list item **1)
        ])))

)